{
  "root": true,
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "babelOptions":{
      "presets": [
        ["babel-preset-react-app",false],
        "babel-preset-react-app/prod"
      ]
    }
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "parser": "@babel/eslint-parser",
  "plugins": [
    "@babel/eslint-plugin"
  ],
  "rules": {
    "react/prop-types": 0
  }
}